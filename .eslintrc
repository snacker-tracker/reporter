{
  "globals": {
    "__VERSION__": true,
    "process": true,
    "fetch": true,
    "afterAll": true,
    "beforeAll": true,
    "afterEach": true,
    "beforeEach": true,
    "context": true,
    "describe": true,
    "expect": true,
    "it": true,
    "jest": true,
    "sinon": true,
    "test": true,
    "jasmine": true,
    "__filename": true,
    "__dirname": true,
    "require": true,
    "navigator": true,
    "window": true
  },
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": [],
  "rules": {
    "no-unsafe-finally": 0,
    "max-len": [
      "error",
      {
        "code": 320
      }
    ],
    "max-lines": [
      "error",
      {
        "max": 1000,
        "skipBlankLines": true
      }
    ],
    "semi": ["error", "never"],
    "no-console": 0,
    "no-unused-vars": 1,
    "no-undef": 2,
    "indent": ["warn", 2, { "SwitchCase": 1 }],
    "key-spacing": [2],
    "object-curly-spacing": [2, "always"],
    "comma-dangle": [
      "error",
      {
        "arrays": "only-multiline",
        "objects": "only-multiline",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ],
    "quotes": [2, "single", "avoid-escape"],
    "import/default": 0,
    "import/named": 0,
    "no-var": "error",
    "id-length": 1
  }
}
