{
  "name": "snacker-tracker-reporter",
  "version": "0.0.1",
  "description": "Tracks snacks taken from the office",
  "main": "index.js",
  "repository": "https://github.com/snacker-tracker/reporter",
  "author": "Laurence MacGuire",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "NODE_ENV='development' ./node_modules/.bin/nodemon -e js,yml -w src --exec \"ts-node src/bin/web.js\"",
    "stream-processor": "NODE_ENV='development' ./node_modules/.bin/nodemon -e js,yml -w src --exec \"ts-node src/bin/stream.js\"",
    "build": "NODE_OPTIONS=--max-old-space-size=16000 tsc",
    "start-web": "node dist/bin/web.js",
    "start-stream": "node dist/bin/stream.js",
    "test": "jest src",
    "test:ci": "jest --coverage src",
    "lint": "eslint src",
    "audit": "yarn audit",
    "knex": "./node_modules/.bin/babel-node ./node_modules/.bin/knex",
    "swagger:deref": "./node_modules/.bin/babel-node ./scripts/swagger-dereference.js",
    "cucumber": "./node_modules/.bin/babel-node ./node_modules/.bin/cucumber-js"
  },
  "eslintIgnore": [
    "src/database/migrations/**/**"
  ],
  "dependencies": {
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "aws-sdk": "^2.814.0",
    "axios": "^0.21.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-jwt": "^8.4",
    "express-jwt-authz": "^2.3.1",
    "express-openapi": "^12.0.1",
    "express-prom-bundle": "^5.1.5",
    "glob": "^10.2.7",
    "jmespath": "^0.16.0",
    "jwks-rsa": "^2.1",
    "knex": "^2.4",
    "multer": "^1.4.5-lts.1",
    "objection": "^2.2.16",
    "openapi-types": "^12.1.3",
    "pg": "^8.0.3",
    "prom-client": "^11.5.3",
    "swagger-client": "^3.18.5",
    "uuid": "^9.0.0",
    "yaml": "^1.7.2"
  },
  "devDependencies": {
    "@apisyouwonthate/style-guide": "^1.4.0",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/core": "^7.22.5",
    "@cucumber/cucumber": "^8.9.1",
    "@stoplight/spectral-cli": "^6.8.0",
    "@stryker-mutator/core": "^5.4.0",
    "@stryker-mutator/jest-runner": "^5.4.0",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/node": "^18",
    "eslint": "^6.8.0",
    "jest": "29.5.0",
    "nodemon": "^2.0.2",
    "stryker-cli": "^1.0.2",
    "swagger-cli": "^4.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
